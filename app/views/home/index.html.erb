<script type="text/javascript">
  vpw = $(window).width();
  vph = $(window).height();
  var windowHeightThreshold = 700;
  var buffer = 200;
  if (vph > windowHeightThreshold) {
    $('#top').height(vph+buffer);
    $('#experience').height(vph+buffer+180);
    $('#recent-publications').height(vph+buffer+650);
    $('#education').height(vph+buffer);
    $('#stack').height(vph+buffer);
    $('#contact').height(vph);
  }
  else {
    $('#top').height(windowHeightThreshold+buffer);
    $('#experience').height(windowHeightThreshold+buffer+180);
    $('#recent-publications').height(windowHeightThreshold+buffer+650);
    $('#education').height(windowHeightThreshold+buffer);
    $('#stack').height(windowHeightThreshold+buffer);
    $('#contact').height(windowHeightThreshold);
  }
</script>
